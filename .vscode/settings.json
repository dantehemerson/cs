{
  "files.associations": {
    "*.mdx": "markdown",
    "ostream": "cpp",
    "new": "cpp",
    "type_traits": "cpp",
    "iomanip": "cpp",
    "iostream": "cpp",
    "vector": "cpp"
  },
  "editor.formatOnSave": false,
  // To support intellisense for C++17:
  "clangd.arguments": [
    "--header-insertion=never",
    "--header-insertion-decorators=0",
    "--std=c++17"
  ]
}
